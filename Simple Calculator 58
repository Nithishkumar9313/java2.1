import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
      Scanner nithish=new Scanner(System.in);
        int a,b;
        a=nithish.nextInt();
        b=nithish.nextInt();
        System.out.println(a+b);
        System.out.println(a-b);
        System.out.println(a*b);
        System.out.println(a/b);
        
    }
}
