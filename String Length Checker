import java.io.*;
import java.util.*;

import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        String S = sc.nextLine();

        if (S == null || S.isEmpty()) {
            System.out.println("Null value found");
        } else {
            System.out.println(S.length());
        }

        sc.close();
    }
}
